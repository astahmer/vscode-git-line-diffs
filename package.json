{
  "name": "vscode-git-status-infos",
  "publisher": "astahmer",
  "displayName": "Git Status Infos",
  "version": "0.0.1",
  "private": true,
  "main": "./out/extension.js",
  "engines": {
		"vscode": "^1.73.0"
	},
  "activationEvents": [
		"*"
	],
  "contributes": {
    "commands": [
      {
        "command": "extension.showChangedFiles",
        "title": "Show Changed Files"
      }
    ]
  },
  "scripts": {
		"vscode:prepublish": "npm run build",
		"build": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "typescript": "^5.7.2"
  }
}
