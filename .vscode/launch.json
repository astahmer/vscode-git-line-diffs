{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
			  "--disable-updates",
			  "--disable-workspace-trust",
			  "--profile-temp",
			  "--skip-release-notes",
			  "--skip-welcome",
			  "--extensionDevelopmentPath=${workspaceFolder}"
			],
			"sourceMaps": true,
			"outFiles": [
			  "${workspaceFolder}/out/**/*.js"
			]
		}
	]
}
